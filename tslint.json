{
    "rules": {
        "class-name": true,
        "comment-format": [
            true,
            "check-space"
        ],
        "no-unused-variable": true,
        "no-use-before-declare": true,
        "forin": true,
        "indent": [
            true,
            "spaces"
        ],
        "linebreak-style": [
            true,
            "LF"
        ],
        "no-duplicate-variable": true,
        "no-eval": true,
        "no-internal-module": true,
        "no-invalid-this": false,
        "no-trailing-whitespace": true,
        "no-var-keyword": true,
        "one-line": [
            true,
            "check-open-brace"
        ],
        "quotemark": [
            true,
            "double"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "typedef-whitespace": [
            false
        ],
        "variable-name": [
            true,
            "ban-keywords"
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-module"
        ]
    }
}
